<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="44f3b084-2fce-4688-957c-1c0348046380" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>&lt;Root project&gt;</name>
  <description>Contains all other projects</description>
  <parameters />
  <project-extensions>
    <extension id="PROJECT_EXT_1" type="ReportTab">
      <parameters>
        <param name="startPage" value="coverage.zip!index.html" />
        <param name="title" value="Code Coverage" />
        <param name="type" value="BuildReportTab" />
      </parameters>
    </extension>
    <extension id="PROJECT_EXT_3" type="versionedSettings">
      <parameters>
        <param name="buildSettings" value="ALWAYS_USE_CURRENT" />
        <param name="credentialsStorageType" value="credentialsJSON" />
        <param name="enabled" value="true" />
        <param name="ignoreChangesInDependenciesAndVcsSettings" value="false" />
        <param name="rootId" value="Teamcity" />
        <param name="showChanges" value="false" />
      </parameters>
    </extension>
  </project-extensions>
  <cleanup>
    <policy type="days" cleanup-level="EVERYTHING">
      <parameters>
        <param name="keepDays.count" value="365" />
      </parameters>
    </policy>
    <policy type="days" cleanup-level="HISTORY_ENTRY">
      <parameters>
        <param name="keepDays.count" value="90" />
      </parameters>
    </policy>
    <options>
      <option name="preventDependenciesArtifactsFromCleanup" value="false" />
    </options>
  </cleanup>
</project>

